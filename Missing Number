class Solution {
    public int missingNumber(int[] nums) {
        Set<Integer> rsNum = new HashSet<>();
        for(int num:nums){
            rsNum.add(num);
        }
        for(int i = 0 ;i <= nums.length;i++){
            if(!rsNum.contains(i)){
                return i;
            }
        }
        return -1;
    }
}
